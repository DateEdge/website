<div class="col-sm-12">
  <div class="page-header">
    <h1>
      Everyone
      <small>search is coming soon</small>
    </h1>
  </div>

  <% @users.in_groups_of(2) do |user| %>
    <div class="row">
      <% unless user.nil? %>
        <div class="col-sm-6">
          <%= render @user %>
        </div>
      <% end %>
    </div>
  <% end %>
</div>

<div class="col-sm-12">
  <%= will_paginate @users %>
</div>
