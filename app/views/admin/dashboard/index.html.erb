<div class='row'>

  <div class='col-sm-4'>
    <h1>2014 Goals</h1>

    <div class='well'>
      <ul>
        <li><s>New Homepage</s></li>
        <li>Map</li>
        <li>Social sidebar 2.0</li>
        <li>DxE Deck&trade;</li>
        <li>iOS app</li>
        <li>&nbsp;</li>
        <li>5,000 users</li>
        <li>Local meetups</li>
      </ul>
    </div>
  </div>

  <%
  {
    "Flagged Users"  => @flagged_users,
    "Flagged Photos" => @flagged_photos
  }.each do |title, collection|
  %>
    <div class="col-sm-4">
      <h1><%= title %></h1>

      <table class='table'>
        <% collection.each do |slug, count| %>
          <%= content_tag "tr", class: red_flag_classes(count) do %>
            <td><%= link_to slug, admin_red_flag_path(slug) %></td>
            <td><%= count %></td>
          <% end %>
        <% end %>
      </table>

    </div>
  <% end %>


</div><!-- .row -->
